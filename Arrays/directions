int x,y;
for(int i=0;i<dir.length;i++){
    if(dir.charAt(i)=='N'){
        y++;
    }
    if(dir.charAt(i)=='E'){
        X++;
    }
    if(dir.charAt(i)=='W'){
        x--;
    }
    if(dir.charAt(i)=='S'){
        y--;
    }
    if(x==0&&y==0)
    return 1;
    else 
    return 0;


}